{% extends 'base.html.twig' %}


{% block body %}

    <h3>Users</h3>

    <table>
        <thead>
        <tr>
            <td>Username</td>
            <td>Email</td>
            <td>Role</td>
            <td>Activ</td>
        </tr>
        </thead>
        <tbody>

        {% for user in users %}
            {{ dump(user) }}
        <tr>
            <td>
                {{ user.username }}
            </td>
            <td>
                {{ user.email }}
            </td>

            <td>

            {% for role in user.roles %}
                    {{ role }}

                {%   endfor %}
            </td>
            <td>
                {{ user.enabled }}
            </td>
        </tr>

        {% endfor %}

        </tbody>

    </table>

{% endblock%}